{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true, 
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true, // jsonファイルから型解決した状態で値を取得でき、json用のinterfaceを作成しなくてよい
    //"noImplicitAny": false, // 暗黙のany型を禁止するコンパイラオプション
  }
}
